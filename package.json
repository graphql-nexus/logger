{
  "name": "floggy",
  "version": "0.0.0",
  "repository": "git@github.com:graphql-nexus/logger.git",
  "author": "Jason Kuhrt",
  "license": "MIT",
  "files": [
    "dist",
    "dist-esm"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist-esm/index.js"
    }
  },
  "main": "dist/index.js",
  "module": "./dist-esm/index.js",
  "scripts": {
    "format": "yarn format:prettier && yarn format:imports",
    "format:prettier": "prettier --write .",
    "format:imports": "format-imports src && format-imports tests",
    "format:check": "yarn -s format:check:imports && yarn -s format:check:prettier",
    "format:check:imports": "format-imports --check src tests",
    "format:check:prettier": "prettier --check .",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "lint:check": "eslint . --ext .ts,.tsx --max-warnings 0",
    "dev": "yarn clean && tsc -w -p tsconfig.cjs.json",
    "dev:test": "yarn test --watch",
    "build": "tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json",
    "test": "jest",
    "clean": "rm -rf dist* *.tsbuildinfo",
    "release:stable": "dripip stable",
    "release:preview": "dripip preview",
    "release:pr": "dripip pr",
    "prepublishOnly": "yarn build"
  },
  "devDependencies": {
    "@prisma-labs/prettier-config": "0.1.0",
    "@types/jest": "26.0.20",
    "@types/lodash": "4.14.168",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "dripip": "0.10.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-only-warn": "^1.0.2",
    "format-imports": "^2.4.0",
    "jest": "26.6.3",
    "jest-watch-suspend": "1.1.2",
    "jest-watch-typeahead": "0.6.1",
    "prettier": "2.2.1",
    "ts-jest": "26.5.1",
    "ts-node": "^9.1.1",
    "type-fest": "0.21.1",
    "typescript": "4.1.5",
    "typescript-snapshots-plugin": "1.7.0"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "fp-ts": "^2.9.5",
    "lodash": "^4.17.21",
    "strip-ansi": "^6.0.0"
  },
  "importSorter": {
    "emptyLinesBetweenGroups": false,
    "wrappingStyle": "prettier"
  }
}
